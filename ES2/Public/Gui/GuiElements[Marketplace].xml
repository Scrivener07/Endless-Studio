        CanBuyInMarketplaceScreen="true" 
        CanSellInMarketplaceScreen="true">
        
        <Title>%TradableCategoryResourceStrategicTitle</Title>
        <Description>%TradableCategoryResourceStrategicDescription</Description>
        <SymbolString>[Strat01titanium]</SymbolString>
        <TableGuiElement Name ="BuyableResourcesItemsTable" Height="42">
            <Column Name="Name"        Width="484" Prefab="Prefabs/Gui/Table/TableCellBuyableName" />
            <Column Name="Quantity"    Width="100" Prefab="Prefabs/Gui/Table/TableCellBuyable" />
            <Column Name="UnitPrice"   Width="182" Prefab="Prefabs/Gui/Table/TableCellBuyablePrice" />
        </TableGuiElement>
    </TradableSectionGuiElement>   

    <TradableSectionGuiElement 
        Name="TradableSectionShip" 
        Order="2"
        CanBuyInMarketplaceScreen="true" 
        CanSellInMarketplaceScreen="false">
        
        <Title>%TradableCategoryShipTitle</Title>
        <Description>%TradableCategoryShipDescription</Description>
        <SymbolString>[ship]</SymbolString>
        <TableGuiElement Name ="BuyableShipsItemsTable" Height="42">
            <Column Name="Name"                      Width="330" Prefab="Prefabs/Gui/Table/TableCellBuyableName" />
            <Column Name="Role"                      Width="42" Prefab="Prefabs/Gui/Table/TableCellBuyableStat" />
            <Column Name="Size"                      Width="42" Prefab="Prefabs/Gui/Table/TableCellBuyableStat" />
            <Column Name="OffensiveMilitaryPower"    Width="42" Prefab="Prefabs/Gui/Table/TableCellBuyableStat" />
            <Column Name="DefensiveMilitaryPower"    Width="42" Prefab="Prefabs/Gui/Table/TableCellBuyableStat" />
            <Column Name="Health"                    Width="42" Prefab="Prefabs/Gui/Table/TableCellBuyableStat" />
            <Column Name="Movement"                  Width="42" Prefab="Prefabs/Gui/Table/TableCellBuyableStat" />
            <Column Name="CommandPoints"             Width="42" Prefab="Prefabs/Gui/Table/TableCellBuyableStat" />
            <Column Name="Manpower"                  Width="42" Prefab="Prefabs/Gui/Table/TableCellBuyableStat" />
            <Column Name="UnitPrice"                 Width="100" Prefab="Prefabs/Gui/Table/TableCellBuyablePrice" />
        </TableGuiElement>
    </TradableSectionGuiElement>

    <TradableSectionGuiElement 
        Name="TradableSectionHero" 
        Order="3" 
        CanBuyInMarketplaceScreen="true" 
        CanSellInMarketplaceScreen="false">
        
        <Title>%TradableCategoryHeroTitle</Title>
        <Description>%TradableCategoryHeroDescription</Description>
