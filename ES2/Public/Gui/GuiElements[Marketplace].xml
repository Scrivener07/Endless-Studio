            <Column Name="Name"             Width="330" Prefab="Prefabs/Gui/Table/TableCellBuyableName" />
            <Column Name="Level"            Width="42" Prefab="Prefabs/Gui/Table/TableCellBuyableStat" />
            <Column Name="Affinity"         Width="42" Prefab="Prefabs/Gui/Table/TableCellHeroPropertySmall" />
            <Column Name="Class"            Width="42" Prefab="Prefabs/Gui/Table/TableCellHeroPropertySmall" />
            <Column Name="Politics"         Width="42" Prefab="Prefabs/Gui/Table/TableCellHeroPoliticsSmall" />
            <Column Name="MasteryWit"       Width="42" Prefab="Prefabs/Gui/Table/TableCellBuyableStat" />
            <Column Name="MasteryLabour"    Width="42" Prefab="Prefabs/Gui/Table/TableCellBuyableStat" />
            <Column Name="MasteryCommand"   Width="42" Prefab="Prefabs/Gui/Table/TableCellBuyableStat" />
