    <!-- In the Description, the following keys can be used to provide precise information: -->
    <!-- $(Turn), $(LastTurn), $(Progress), $(Score), $(Rank) -->
    
    <VictoryConditionGuiElement Name="VictoryScore">
        <Title>%VictoryScoreTitle</Title>
        <Description>%VictoryScoreDescription</Description>
