    <!-- ************************ -->

    <GuiElement Name="GuiFleetGroup">
        <Icons>
            <Icon Size="Small" Path="Bitmaps/Atlased/Headers/HeaderFleet" />
        </Icons>
    </GuiElement>
    
    <!-- ************************ -->
    <!-- ** Fleet statuses ** -->
    <!-- ************************ -->

    <GuiElement Name="FleetStatusOrbiting">
