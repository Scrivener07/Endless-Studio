  <WorldSettingDefinition Name="EmpireSpawnMethod"  Type="string"   Default="Standard">
    
    <ItemDefinition Name="Standard">
      <KeyValuePair Key="Warp">1</KeyValuePair>
      <KeyValuePair Key="MajorNeighbour">-7</KeyValuePair>
      <KeyValuePair Key="LinkDistanceToNearestMajor">1</KeyValuePair>
      <KeyValuePair Key="EuclidianDistanceToNearestMajor">1</KeyValuePair>
      <!--KeyValuePair Key="TooCloseToNearestMajor">-7</KeyValuePair-->
      <!--KeyValuePair Key="TooCloseDistance">15.0</KeyValuePair-->
      <KeyValuePair Key="MajorInSameConstellation">-1000</KeyValuePair>
      <KeyValuePair Key="CatchmentArea">1</KeyValuePair>
    </ItemDefinition>
    
    <ItemDefinition Name="Chaotic"/>

    <ItemDefinition Name="Clustered">
      <KeyValuePair Key="Warp">1</KeyValuePair>
      <KeyValuePair Key="MajorNeighbour">3</KeyValuePair>
      <KeyValuePair Key="LinkDistanceToNearestMajor">-1</KeyValuePair>
      <KeyValuePair Key="EuclidianDistanceToNearestMajor">-0.1</KeyValuePair>
      <!--KeyValuePair Key="TooCloseToNearestMajor">7</KeyValuePair-->
      <!--KeyValuePair Key="TooCloseDistance">15.0</KeyValuePair-->
      <KeyValuePair Key="MajorInSameConstellation">5</KeyValuePair>
      <KeyValuePair Key="CatchmentArea">1</KeyValuePair>
    </ItemDefinition>
    
  </WorldSettingDefinition>

