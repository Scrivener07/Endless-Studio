		<CustomCost ResourceName="EmpireMoney" Instant="true">Property(StockLocation,@ClassEmpire,DiplomacyCostReduction) * 35 * Property(StockLocation,@ClassEmpire,PeacefulTreatyCostReduction) * Property(StockLocation,@ClassEmpire,SystemCount)</CustomCost>

		<PathPrerequisite Flags="Prerequisite,Discard" Inverted="true">ClassEmpire,AffinityUnfallen</PathPrerequisite>
