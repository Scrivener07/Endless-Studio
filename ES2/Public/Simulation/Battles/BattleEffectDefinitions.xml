	<BattleEffect_SetPropertyValue Name="Squadron_RemoveDamageAbsorbedByHullPlatingFromOverallDamage"   PropertyName="LastAttackOverallDamage"         Operation="Subtraction"     InterpreterFormula="Property(Initiator, @ClassModuleSquadronEncounter, DamageAbsorbedByHullPlating)"/>
