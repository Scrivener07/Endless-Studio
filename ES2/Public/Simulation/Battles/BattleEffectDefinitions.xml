    <BattleEffect_SetPropertyValue Name="Squadron_ComputeOverallDamageAgainstShip"         PropertyName="LastAttackOverallDamage"   Operation="Set" InterpreterFormula="(Property(Initiator, @ClassModuleSquadron, Damage) * (Property(Initiator, @ClassModuleSquadronEncounter, RemainingUnitsCount) / Property(Initiator, @ClassModuleSquadron, BaseUnitsCount)) * (1 + Property(Initiator, @ClassModuleSquadronEncounter, LastAttackCriticalAttack))) * (1 - Property(InitiatorTarget, @ClassShip, DefenseBonusOverall)) * (Property(Initiator, @ClassModuleSquadron, DamageAgainstSmallShipsModifier) * Count(InitiatorTarget, @'ClassShip,ShipSizeSmall') + Property(Initiator, @ClassModuleSquadron, DamageAgainstMediumShipsModifier) * Count(InitiatorTarget, @'ClassShip,ShipSizeMedium') + Property(Initiator, @ClassModuleSquadron, DamageAgainstLargeShipsModifier) * Count(InitiatorTarget, @'ClassShip,ShipSizeLarge') + Property(Initiator, @ClassModuleSquadron, DamageAgainstLargeShipsModifier) * Count(InitiatorTarget, @'ClassShip,ShipSizeMothership'))" />
    <BattleEffect_SetPropertyValue Name="Squadron_ComputeOverallDamageAgainstSquadron"         PropertyName="LastAttackOverallDamage"   Operation="Set" InterpreterFormula="(Property(Initiator, @ClassModuleSquadron, Damage) * (Property(Initiator, @ClassModuleSquadronEncounter, RemainingUnitsCount) / Property(Initiator, @ClassModuleSquadron, BaseUnitsCount)) * (1 + Property(Initiator, @ClassModuleSquadronEncounter, LastAttackCriticalAttack))) / Property(InitiatorTarget, @ClassModuleSquadron, SquadronDefense)" />
	<BattleEffect_SetPropertyValue Name="Squadron_ApplySquadronDodgeToOverallDamage" PropertyName="LastAttackOverallDamage"   Operation="Subtraction" InterpreterFormula="Min(Property(Initiator, @ClassModuleSquadronEncounter, LastAttackOverallDamage), (Property(Initiator, @ClassModuleSquadronEncounter, LastAttackOverallDamage) * Property(InitiatorTarget, @ClassModuleSquadronEncounter, LastSquadronAttackCriticalDodge)) + (Property(Initiator, @ClassModuleSquadronEncounter, LastAttackOverallDamage) * Property(InitiatorTarget, @ClassModuleSquadron, DodgeChance)))" />
	<BattleEffect_SetPropertyValue Name="Squadron_ApplyEffectSquadronDamageIncreasePerSquadron" PropertyName="LastAttackOverallDamage" Operation="Set" InterpreterFormula="Property(Initiator, @ClassModuleSquadronEncounter, LastAttackOverallDamage) + Property(Initiator, @ClassModuleSquadronEncounter, LastAttackOverallDamage) * (Property(Initiator, @'../ClassGroup', SquadronAmount)/100) * Property(Initiator, @'../ClassGroup', HasEffectSquadronDamageIncreasePerSquadron)"/>

