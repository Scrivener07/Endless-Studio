		<!--<HappinessStatusSet Name="HappinessStatusSetPopulationDefault"/>-->

        <PlanetScoreFormulae>
			<PlanetScoreFormula>
				<PathPrerequisite>ClassPopulation,PopulationParasite</PathPrerequisite>
				<Formula>((Property(Planet, @ClassPlanet, PlanetFood) * 5.0) + (Property(Planet, @ClassPlanet, PlanetIndustry) * 2.0) + (Property(Planet, @ClassPlanet, PlanetDust) * 1.5) + (Property(Planet, @ClassPlanet, PlanetScience) * 1.5) + Property(Planet, @ClassPlanet, PlanetPrestige)) / (11.0 * (Max(1, Property(Planet, @ClassPlanet, Population)))) + Property(Planet, @ClassPlanet, ParasitedPopulation) * 5</Formula>
			</PlanetScoreFormula>
		</PlanetScoreFormulae>
        
